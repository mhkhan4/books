echo +++++++whoami++++++++
whoami

echo +++++++Stop Service+++++++
sudo systemctl stop tftimr.service
sudo systemctl stop tftimr2.service

echo +++++++Copy files++++++++
cp "$(System.DefaultWorkingDirectory)/Dev/drop/target/itlmqreceiver-1.0.0.jar" /opt/tft/itlmqreceiver1/itlmqreceiver.jar
cp "$(System.DefaultWorkingDirectory)/Dev/drop/target/classes/application-dev.properties" /opt/tft/itlmqreceiver1/application-dev.properties
cp "$(System.DefaultWorkingDirectory)/Dev/drop/target/classes/logback-spring.xml" /opt/tft/itlmqreceiver1/logback-spring.xml

echo +++++++Copy files++++++++
cp "$(System.DefaultWorkingDirectory)/Dev/drop/target/itlmqreceiver-1.0.0.jar" /opt/tft/itlmqreceiver2/itlmqreceiver.jar
cp "$(System.DefaultWorkingDirectory)/Dev/drop/target/classes/application-dev.properties" /opt/tft/itlmqreceiver2/application-dev.properties
cp "$(System.DefaultWorkingDirectory)/Dev/drop/target/classes/logback-spring.xml" /opt/tft/itlmqreceiver2/logback-spring.xml

echo +++++++List folder++++++++
ls -lart /opt/tft/itlmqreceiver1
ls -lart /opt/tft/itlmqreceiver2

echo +++++++Start Service+++++++
sudo systemctl start tftimr.service
sudo systemctl start tftimr2.service

echo +++++++Done+++++++




